@inherits UmbracoViewPage<PageViewModel>
<!DOCTYPE html>
<html lang="en">
<head>
    <meta content="text/html; charset=utf-8" http-equiv="Content-Type" />
    <title>
        Lebo
    </title>
    <meta content="Lebo" property="og:title" />
    <meta content="https://cdn.prod.website-files.com/686ed6946d8d058c103c1e28/686ed7d732898887a9fcbf1c_%2B971553094460.zip%20-%202.webp" property="og:image" />
    <meta content="Lebo" property="twitter:title" />
    <meta content="https://cdn.prod.website-files.com/686ed6946d8d058c103c1e28/686ed7d732898887a9fcbf1c_%2B971553094460.zip%20-%202.webp" property="twitter:image" />
    <meta content="width=device-width, initial-scale=1" name="viewport" />
    <link href="https://fonts.googleapis.com/" rel="preconnect" />
    <link crossorigin="anonymous" href="https://fonts.gstatic.com/" rel="preconnect" />
    <link href="https://cdn.prod.website-files.com/686ed6946d8d058c103c1e28/6875fa0ed78811eca084b75e_favicon.png" rel="shortcut icon" type="image/x-icon" />
    <link href="https://cdn.prod.website-files.com/686ed6946d8d058c103c1e28/6875fa10fd0f966f44d05040_webclip.png" rel="apple-touch-icon" />
    <link rel="stylesheet" href="css/animations.css" />
    @await RenderSectionAsync("Head", required: false)
</head>
<body>
    <div class="page-wrapper">
        @{
            await Html.RenderPartialAsync("_Navbar");
        }
        <main class="main-wrapper">
            @RenderBody()
        </main>

        @{
            await Html.RenderPartialAsync("_Footer");
        }
    </div>

    <link href="styles.css" rel="stylesheet" />
    <link href="shared-styles.css" rel="stylesheet" />
    <link href="fonts.css" rel="stylesheet" />
    <script src="https://d3e54v103j8qbb.cloudfront.net/js/jquery-3.5.1.min.dc5e7f18c8.js"></script>
    <script src="scripts.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
